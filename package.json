{
    "name": "x86",
    "displayName": "x86 Instruction Reference",
    "description": "Intel x86 instruction reference in a VS Code custom Activity Bar view",
    "version": "0.5.3",
    "publisher": "laefad",
    "contributors": [
        "rgosens2",
        "whiteout2"
    ],
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/laefad/x86.git"
    },
    "icon": "media/icons/x86_white_128.png",
    "license": "MIT",
    "main": "./dist/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "x86-instructions-explorer",
                    "title": "x86 Instructions",
                    "icon": "media/icons/x86_white_128.png"
                }
            ]
        },
        "views": {
            "x86-instructions-explorer": [
                {
                    "id": "instructionsList",
                    "type": "webview",
                    "name": "instructionsList"
                }
            ]
        },
        "commands": [
            {
                "command": "openInstructionView",
                "title": "Open instruction view for selected asm instruction"
            },
            {
                "command": "updateInstructionsList",
                "title": "Update instructions list"
            }
        ],
        "configuration": []
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "vscode:package": "vsce package",
        "webpack": "webpack --mode development",
        "webpack-dev": "webpack --mode development --watch",
        "package": "webpack --mode production --devtool hidden-source-map"
    },
    "devDependencies": {
        "@types/jsdom": "21.1.6",
        "@types/node": "20.11.10",
        "@types/vscode": "1.85.0",
        "@types/vscode-webview": "1.57.4",
        "@types/webpack": "5.28.5",
        "@types/webpack-node-externals": "3.0.4",
        "@vscode/vsce": "2.23.0",
        "copy-webpack-plugin": "12.0.2",
        "ts-loader": "9.5.1",
        "ts-node": "10.9.2",
        "tsconfig-paths-webpack-plugin": "4.1.0",
        "typescript": "5.3.3",
        "webpack": "5.90.0",
        "webpack-cli": "5.1.4"
    },
    "dependencies": {
        "@vscode-elements/elements": "1.0.1",
        "@vscode/codicons": "0.0.35",
        "@vscode/webview-ui-toolkit": "1.4.0",
        "jsdom": "24.0.0"
    }
}
